{
  "include": [".*.js", "*.js", "api"],
  "exclude": [],
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "jsx": "react",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./out",
    "rootDir": "./api",
    "isolatedModules": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {},
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
